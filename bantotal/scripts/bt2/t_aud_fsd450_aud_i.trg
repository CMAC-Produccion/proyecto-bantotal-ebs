CREATE OR REPLACE TRIGGER T_AUD_FSD450_AUD_I  AFTER INSERT ON FSD450  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FSD450_AUDIT ( AUD_new_CBIEANT,AUD_new_CBIECTA,AUD_new_CBIEEMP,AUD_new_CBIEFEC,AUD_new_CBIEMDA,AUD_new_CBIEMOD,AUD_new_CBIEOPE,AUD_new_CBIEPAP,AUD_new_CBIEREL,AUD_new_CBIESUB,AUD_new_CBIESUC,AUD_new_CBIETOP,AUD_new_CBIETXT1,AUD_new_CBIETXT2,AUD_new_CBIETXT3,AUD_new_EXCOD,AUD_new_HCMOD,AUD_new_HCORD,AUD_new_HCSUBO,AUD_new_HFCONT,AUD_new_HNREL,AUD_new_HSUCOR,AUD_new_HTRAN,AUD_new_PGCOD, AUD_FSD450_GuidKey ,AUD_FSD450_UOn,AUD_FSD450_UT ,AUD_FSD450_UBR ,AUD_FSD450_UBC ,AUD_FSD450_UBT ,AUD_FSD450_UBU ,AUD_FSD450_UBA ,AUD_FSD450_UBS ,AUD_FSD450_UBP ,AUD_FSD450_UBM ,AUD_FSD450_UAs  )      VALUES (:new.CBIEANT,:new.CBIECTA,:new.CBIEEMP,:new.CBIEFEC,:new.CBIEMDA,:new.CBIEMOD,:new.CBIEOPE,:new.CBIEPAP,:new.CBIEREL,:new.CBIESUB,:new.CBIESUC,:new.CBIETOP,:new.CBIETXT1,:new.CBIETXT2,:new.CBIETXT3,:new.EXCOD,:new.HCMOD,:new.HCORD,:new.HCSUBO,:new.HFCONT,:new.HNREL,:new.HSUCOR,:new.HTRAN,:new.PGCOD, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'I'); END T_AUD_FSD450_AUD_I;
/

