CREATE OR REPLACE TRIGGER T_AUD_X054011_AUD_I  AFTER INSERT ON X054011  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_X054011_AUDIT ( AUD_new_PGCOD,AUD_new_SGCOD,AUD_new_XPREMOD,AUD_new_XPREMONEDA,AUD_new_XPREPAPEL,AUD_new_XPRETOPE,AUD_new_XSGAUX1,AUD_new_XSGAUX2,AUD_new_XSGAUX3,AUD_new_XSGAUX4,AUD_new_XSGENDO,AUD_new_XSGMODIF, AUD_X054011_GuidKey ,AUD_X054011_UOn,AUD_X054011_UT ,AUD_X054011_UBR ,AUD_X054011_UBC ,AUD_X054011_UBT ,AUD_X054011_UBU ,AUD_X054011_UBA ,AUD_X054011_UBS ,AUD_X054011_UBP ,AUD_X054011_UBM ,AUD_X054011_UAs  )      VALUES (:new.PGCOD,:new.SGCOD,:new.XPREMOD,:new.XPREMONEDA,:new.XPREPAPEL,:new.XPRETOPE,:new.XSGAUX1,:new.XSGAUX2,:new.XSGAUX3,:new.XSGAUX4,:new.XSGENDO,:new.XSGMODIF, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'I'); END T_AUD_X054011_AUD_I;
/

