CREATE OR REPLACE PROCEDURE AUD_C_SNG404_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_SNG404_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_SNG404_UOn,AUD_SNG404_GuidKey,'SNG404',CONCAT( CONCAT( CONCAT( CONCAT( '|SNG400COD=', RTRIM(COALESCE(AUD_old_SNG400COD,AUD_new_SNG400COD)) ),CONCAT( '|SNG400EVTO=', RTRIM(COALESCE(AUD_old_SNG400EVTO,AUD_new_SNG400EVTO)) ) ),CONCAT( '|SNG404ST=', RTRIM(COALESCE(AUD_old_SNG404ST,AUD_new_SNG404ST)) ) ),'|' ),AUD_SNG404_UBT,AUD_SNG404_UBU,AUD_SNG404_UBM,AUD_SNG404_UBP,AUD_SNG404_UBR,AUD_SNG404_UAs,AUD_SNG404_UBC,AUD_SNG404_UBA,AUD_SNG404_UBS, '', 'BANTOTAL', '', '', '',AUD_SNG404_UT FROM BANTPROD.AUD_SNG404_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG404_UOn AND AUDMstId = AUD_SNG404_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG404_UOn,AUD_SNG404_GuidKey,'SNG400COD',AUD_old_SNG400COD,AUD_new_SNG400COD FROM BANTPROD.AUD_SNG404_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG404_UOn AND AUDMstId = AUD_SNG404_GuidKey AND AUDMstFld = 'SNG400COD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG404_UOn,AUD_SNG404_GuidKey,'SNG400EVTO',AUD_old_SNG400EVTO,AUD_new_SNG400EVTO FROM BANTPROD.AUD_SNG404_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG404_UOn AND AUDMstId = AUD_SNG404_GuidKey AND AUDMstFld = 'SNG400EVTO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG404_UOn,AUD_SNG404_GuidKey,'SNG404ST',AUD_old_SNG404ST,AUD_new_SNG404ST FROM BANTPROD.AUD_SNG404_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG404_UOn AND AUDMstId = AUD_SNG404_GuidKey AND AUDMstFld = 'SNG404ST'); DELETE FROM AUD_SNG404_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG404_UOn AND AUDMstId = AUD_SNG404_GuidKey);  COMMIT ;END IF;END;
