CREATE OR REPLACE PROCEDURE AUD_C_FSR005_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_FSR005_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_FSR005_UOn,AUD_FSR005_GuidKey,'FSR005',CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( '|DOCOD=', RTRIM(COALESCE(AUD_old_DOCOD,AUD_new_DOCOD)) ),CONCAT( '|DOORDP=', RTRIM(COALESCE(AUD_old_DOORDP,AUD_new_DOORDP)) ) ),CONCAT( '|PENDOC=', RTRIM(COALESCE(AUD_old_PENDOC,AUD_new_PENDOC)) ) ),CONCAT( '|PEPAIS=', RTRIM(COALESCE(AUD_old_PEPAIS,AUD_new_PEPAIS)) ) ),CONCAT( '|PETDOC=', RTRIM(COALESCE(AUD_old_PETDOC,AUD_new_PETDOC)) ) ),'|' ),AUD_FSR005_UBT,AUD_FSR005_UBU,AUD_FSR005_UBM,AUD_FSR005_UBP,AUD_FSR005_UBR,AUD_FSR005_UAs,AUD_FSR005_UBC,AUD_FSR005_UBA,AUD_FSR005_UBS, '', 'BANTOTAL', '', '', '',AUD_FSR005_UT FROM BANTPROD.AUD_FSR005_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FSR005_UOn AND AUDMstId = AUD_FSR005_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR005_UOn,AUD_FSR005_GuidKey,'DOCOD',AUD_old_DOCOD,AUD_new_DOCOD FROM BANTPROD.AUD_FSR005_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR005_UOn AND AUDMstId = AUD_FSR005_GuidKey AND AUDMstFld = 'DOCOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR005_UOn,AUD_FSR005_GuidKey,'DOFAXP',AUD_old_DOFAXP,AUD_new_DOFAXP FROM BANTPROD.AUD_FSR005_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR005_UOn AND AUDMstId = AUD_FSR005_GuidKey AND AUDMstFld = 'DOFAXP'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR005_UOn,AUD_FSR005_GuidKey,'DOORDP',AUD_old_DOORDP,AUD_new_DOORDP FROM BANTPROD.AUD_FSR005_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR005_UOn AND AUDMstId = AUD_FSR005_GuidKey AND AUDMstFld = 'DOORDP'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR005_UOn,AUD_FSR005_GuidKey,'DOTELFP',AUD_old_DOTELFP,AUD_new_DOTELFP FROM BANTPROD.AUD_FSR005_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR005_UOn AND AUDMstId = AUD_FSR005_GuidKey AND AUDMstFld = 'DOTELFP'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR005_UOn,AUD_FSR005_GuidKey,'DOTLEXP',AUD_old_DOTLEXP,AUD_new_DOTLEXP FROM BANTPROD.AUD_FSR005_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR005_UOn AND AUDMstId = AUD_FSR005_GuidKey AND AUDMstFld = 'DOTLEXP'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR005_UOn,AUD_FSR005_GuidKey,'PENDOC',AUD_old_PENDOC,AUD_new_PENDOC FROM BANTPROD.AUD_FSR005_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR005_UOn AND AUDMstId = AUD_FSR005_GuidKey AND AUDMstFld = 'PENDOC'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR005_UOn,AUD_FSR005_GuidKey,'PEPAIS',AUD_old_PEPAIS,AUD_new_PEPAIS FROM BANTPROD.AUD_FSR005_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR005_UOn AND AUDMstId = AUD_FSR005_GuidKey AND AUDMstFld = 'PEPAIS'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR005_UOn,AUD_FSR005_GuidKey,'PETDOC',AUD_old_PETDOC,AUD_new_PETDOC FROM BANTPROD.AUD_FSR005_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR005_UOn AND AUDMstId = AUD_FSR005_GuidKey AND AUDMstFld = 'PETDOC'); DELETE FROM AUD_FSR005_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FSR005_UOn AND AUDMstId = AUD_FSR005_GuidKey);  COMMIT ;END IF;END;
