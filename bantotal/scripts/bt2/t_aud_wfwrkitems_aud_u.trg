CREATE OR REPLACE TRIGGER T_AUD_WFWRKITEMS_AUD_U  AFTER UPDATE ON WFWRKITEMS  FOR EACH ROW
DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;     INSERT INTO BANTPROD.AUD_WFWRKITEMS_AUDIT (AUD_new_WFINSPRCID,AUD_new_WFITEMDLN,AUD_new_WFITEMDLNTIME,AUD_new_WFITEMEND,AUD_new_WFITEMID,AUD_new_WFITEMINIT,AUD_new_WFITEMPRCURN,AUD_new_WFITEMPRVSTA,AUD_new_WFITEMPRVTASK,AUD_new_WFITEMPTY,AUD_new_WFITEMROLCOD,AUD_new_WFITEMSTSACT,AUD_new_WFITEMUSRCOD,AUD_new_WFITEMWRN,AUD_new_WFITEMWRNTIME,AUD_new_WFPRCID,AUD_new_WFSTSCOD,AUD_new_WFTASKCOD, AUD_old_WFINSPRCID,AUD_old_WFITEMDLN,AUD_old_WFITEMDLNTIME,AUD_old_WFITEMEND,AUD_old_WFITEMID,AUD_old_WFITEMINIT,AUD_old_WFITEMPRCURN,AUD_old_WFITEMPRVSTA,AUD_old_WFITEMPRVTASK,AUD_old_WFITEMPTY,AUD_old_WFITEMROLCOD,AUD_old_WFITEMSTSACT,AUD_old_WFITEMUSRCOD,AUD_old_WFITEMWRN,AUD_old_WFITEMWRNTIME,AUD_old_WFPRCID,AUD_old_WFSTSCOD,AUD_old_WFTASKCOD, AUD_WFWRKITEMS_GuidKey ,AUD_WFWRKITEMS_UOn,AUD_WFWRKITEMS_UT ,AUD_WFWRKITEMS_UBR ,AUD_WFWRKITEMS_UBC ,AUD_WFWRKITEMS_UBT ,AUD_WFWRKITEMS_UBU ,AUD_WFWRKITEMS_UBA ,AUD_WFWRKITEMS_UBS ,AUD_WFWRKITEMS_UBP ,AUD_WFWRKITEMS_UBM ,AUD_WFWRKITEMS_UAs )     VALUES (:new.WFINSPRCID,:new.WFITEMDLN,:new.WFITEMDLNTIME,:new.WFITEMEND,:new.WFITEMID,:new.WFITEMINIT,:new.WFITEMPRCURN,:new.WFITEMPRVSTA,:new.WFITEMPRVTASK,:new.WFITEMPTY,:new.WFITEMROLCOD,:new.WFITEMSTSACT,:new.WFITEMUSRCOD,:new.WFITEMWRN,:new.WFITEMWRNTIME,:new.WFPRCID,:new.WFSTSCOD,:new.WFTASKCOD,  :old.WFINSPRCID,:old.WFITEMDLN,:old.WFITEMDLNTIME,:old.WFITEMEND,:old.WFITEMID,:old.WFITEMINIT,:old.WFITEMPRCURN,:old.WFITEMPRVSTA,:old.WFITEMPRVTASK,:old.WFITEMPTY,:old.WFITEMROLCOD,:old.WFITEMSTSACT,:old.WFITEMUSRCOD,:old.WFITEMWRN,:old.WFITEMWRNTIME,:old.WFPRCID,:old.WFSTSCOD,:old.WFTASKCOD,  SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'U' ); END T_AUD_WFWRKITEMS_AUD_U;
