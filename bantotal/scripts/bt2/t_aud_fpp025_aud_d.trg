CREATE OR REPLACE TRIGGER T_AUD_FPP025_AUD_D  AFTER DELETE ON FPP025  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FPP025_AUDIT (AUD_old_PP010PRD,AUD_old_PP017PAR,AUD_old_PP025AUX1,AUD_old_PP025AUX2,AUD_old_PP025AUX3,AUD_old_PP025AUX4,AUD_old_PP025AUX5,AUD_old_PP025CAR1,AUD_old_PP025CAR2,AUD_old_PP025CAR3,AUD_old_PP025CLAV,AUD_old_PP025DEFC,AUD_old_PP025DEFD,AUD_old_PP025DEFN,AUD_old_PP025DEFV,AUD_old_PP025EMP,AUD_old_PP025MAXN,AUD_old_PP025MINN,AUD_old_PP025MOD1,AUD_old_PP025MOD2,AUD_old_PP025MOD3,AUD_old_PP025TIPOS,AUD_old_PP025VIS1,AUD_old_PP025VIS2,AUD_old_PP025VIS3, AUD_FPP025_GuidKey ,AUD_FPP025_UOn,AUD_FPP025_UT ,AUD_FPP025_UBR ,AUD_FPP025_UBC ,AUD_FPP025_UBT ,AUD_FPP025_UBU ,AUD_FPP025_UBA ,AUD_FPP025_UBS ,AUD_FPP025_UBP ,AUD_FPP025_UBM ,AUD_FPP025_UAs )      VALUES (:old.PP010PRD,:old.PP017PAR,:old.PP025AUX1,:old.PP025AUX2,:old.PP025AUX3,:old.PP025AUX4,:old.PP025AUX5,:old.PP025CAR1,:old.PP025CAR2,:old.PP025CAR3,:old.PP025CLAV,:old.PP025DEFC,:old.PP025DEFD,:old.PP025DEFN,:old.PP025DEFV,:old.PP025EMP,:old.PP025MAXN,:old.PP025MINN,:old.PP025MOD1,:old.PP025MOD2,:old.PP025MOD3,:old.PP025TIPOS,:old.PP025VIS1,:old.PP025VIS2,:old.PP025VIS3, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'D' ); END T_AUD_FPP025_AUD_D;
