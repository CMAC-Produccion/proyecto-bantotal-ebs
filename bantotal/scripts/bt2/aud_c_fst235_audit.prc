CREATE OR REPLACE PROCEDURE AUD_C_FST235_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_FST235_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_FST235_UOn,AUD_FST235_GuidKey,'FST235',CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( '|PGCOD=', RTRIM(COALESCE(AUD_old_PGCOD,AUD_new_PGCOD)) ),CONCAT( '|TRMOD=', RTRIM(COALESCE(AUD_old_TRMOD,AUD_new_TRMOD)) ) ),CONCAT( '|TRNRO=', RTRIM(COALESCE(AUD_old_TRNRO,AUD_new_TRNRO)) ) ),CONCAT( '|TRORD=', RTRIM(COALESCE(AUD_old_TRORD,AUD_new_TRORD)) ) ),CONCAT( '|TXCOD=', RTRIM(COALESCE(AUD_old_TXCOD,AUD_new_TXCOD)) ) ),'|' ),AUD_FST235_UBT,AUD_FST235_UBU,AUD_FST235_UBM,AUD_FST235_UBP,AUD_FST235_UBR,AUD_FST235_UAs,AUD_FST235_UBC,AUD_FST235_UBA,AUD_FST235_UBS, '', 'BANTOTAL', '', '', '',AUD_FST235_UT FROM BANTPROD.AUD_FST235_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST235_UOn AND AUDMstId = AUD_FST235_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST235_UOn,AUD_FST235_GuidKey,'PGCOD',AUD_old_PGCOD,AUD_new_PGCOD FROM BANTPROD.AUD_FST235_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST235_UOn AND AUDMstId = AUD_FST235_GuidKey AND AUDMstFld = 'PGCOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST235_UOn,AUD_FST235_GuidKey,'TRMOD',AUD_old_TRMOD,AUD_new_TRMOD FROM BANTPROD.AUD_FST235_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST235_UOn AND AUDMstId = AUD_FST235_GuidKey AND AUDMstFld = 'TRMOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST235_UOn,AUD_FST235_GuidKey,'TRNRO',AUD_old_TRNRO,AUD_new_TRNRO FROM BANTPROD.AUD_FST235_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST235_UOn AND AUDMstId = AUD_FST235_GuidKey AND AUDMstFld = 'TRNRO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST235_UOn,AUD_FST235_GuidKey,'TRORD',AUD_old_TRORD,AUD_new_TRORD FROM BANTPROD.AUD_FST235_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST235_UOn AND AUDMstId = AUD_FST235_GuidKey AND AUDMstFld = 'TRORD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST235_UOn,AUD_FST235_GuidKey,'TXCOD',AUD_old_TXCOD,AUD_new_TXCOD FROM BANTPROD.AUD_FST235_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST235_UOn AND AUDMstId = AUD_FST235_GuidKey AND AUDMstFld = 'TXCOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST235_UOn,AUD_FST235_GuidKey,'TXECTA',AUD_old_TXECTA,AUD_new_TXECTA FROM BANTPROD.AUD_FST235_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST235_UOn AND AUDMstId = AUD_FST235_GuidKey AND AUDMstFld = 'TXECTA'); DELETE FROM AUD_FST235_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST235_UOn AND AUDMstId = AUD_FST235_GuidKey);  COMMIT ;END IF;END;
