CREATE OR REPLACE PROCEDURE AUD_C_FST050_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_FST050_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_FST050_UOn,AUD_FST050_GuidKey,'FST050',CONCAT( CONCAT( '|COCOD=', RTRIM(COALESCE(AUD_old_COCOD,AUD_new_COCOD)) ),'|' ),AUD_FST050_UBT,AUD_FST050_UBU,AUD_FST050_UBM,AUD_FST050_UBP,AUD_FST050_UBR,AUD_FST050_UAs,AUD_FST050_UBC,AUD_FST050_UBA,AUD_FST050_UBS, '', 'BANTOTAL', '', '', '',AUD_FST050_UT FROM BANTPROD.AUD_FST050_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST050_UOn AND AUDMstId = AUD_FST050_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST050_UOn,AUD_FST050_GuidKey,'COCOD',AUD_old_COCOD,AUD_new_COCOD FROM BANTPROD.AUD_FST050_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST050_UOn AND AUDMstId = AUD_FST050_GuidKey AND AUDMstFld = 'COCOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST050_UOn,AUD_FST050_GuidKey,'CONOM',AUD_old_CONOM,AUD_new_CONOM FROM BANTPROD.AUD_FST050_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST050_UOn AND AUDMstId = AUD_FST050_GuidKey AND AUDMstFld = 'CONOM'); DELETE FROM AUD_FST050_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST050_UOn AND AUDMstId = AUD_FST050_GuidKey);  COMMIT ;END IF;END;
