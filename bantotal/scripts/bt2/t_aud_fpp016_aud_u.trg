CREATE OR REPLACE TRIGGER T_AUD_FPP016_AUD_U  AFTER UPDATE ON FPP016  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;     INSERT INTO BANTPROD.AUD_FPP016_AUDIT (AUD_new_PP010PRD,AUD_new_PP016CAR1,AUD_new_PP016CAR2,AUD_new_PP016CAR3,AUD_new_PP016COD,AUD_new_PP016DSC,AUD_new_PP016EMP,AUD_new_PP016FEC1,AUD_new_PP016FEC2,AUD_new_PP016FEC3,AUD_new_PP016IMP1,AUD_new_PP016IMP2,AUD_new_PP016IMP3,AUD_new_PP016MOD,AUD_new_PP016NUM1,AUD_new_PP016NUM2,AUD_new_PP016NUM3,AUD_new_PP016ORD,AUD_new_PP016TIPO,AUD_new_PP017PAR, AUD_old_PP010PRD,AUD_old_PP016CAR1,AUD_old_PP016CAR2,AUD_old_PP016CAR3,AUD_old_PP016COD,AUD_old_PP016DSC,AUD_old_PP016EMP,AUD_old_PP016FEC1,AUD_old_PP016FEC2,AUD_old_PP016FEC3,AUD_old_PP016IMP1,AUD_old_PP016IMP2,AUD_old_PP016IMP3,AUD_old_PP016MOD,AUD_old_PP016NUM1,AUD_old_PP016NUM2,AUD_old_PP016NUM3,AUD_old_PP016ORD,AUD_old_PP016TIPO,AUD_old_PP017PAR, AUD_FPP016_GuidKey ,AUD_FPP016_UOn,AUD_FPP016_UT ,AUD_FPP016_UBR ,AUD_FPP016_UBC ,AUD_FPP016_UBT ,AUD_FPP016_UBU ,AUD_FPP016_UBA ,AUD_FPP016_UBS ,AUD_FPP016_UBP ,AUD_FPP016_UBM ,AUD_FPP016_UAs )     VALUES (:new.PP010PRD,:new.PP016CAR1,:new.PP016CAR2,:new.PP016CAR3,:new.PP016COD,:new.PP016DSC,:new.PP016EMP,:new.PP016FEC1,:new.PP016FEC2,:new.PP016FEC3,:new.PP016IMP1,:new.PP016IMP2,:new.PP016IMP3,:new.PP016MOD,:new.PP016NUM1,:new.PP016NUM2,:new.PP016NUM3,:new.PP016ORD,:new.PP016TIPO,:new.PP017PAR,  :old.PP010PRD,:old.PP016CAR1,:old.PP016CAR2,:old.PP016CAR3,:old.PP016COD,:old.PP016DSC,:old.PP016EMP,:old.PP016FEC1,:old.PP016FEC2,:old.PP016FEC3,:old.PP016IMP1,:old.PP016IMP2,:old.PP016IMP3,:old.PP016MOD,:old.PP016NUM1,:old.PP016NUM2,:old.PP016NUM3,:old.PP016ORD,:old.PP016TIPO,:old.PP017PAR,  SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'U' ); END T_AUD_FPP016_AUD_U;
