CREATE OR REPLACE PROCEDURE AUD_C_FST056_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_FST056_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_FST056_UOn,AUD_FST056_GuidKey,'FST056',CONCAT( CONCAT( CONCAT( CONCAT( '|CTNRO=', RTRIM(COALESCE(AUD_old_CTNRO,AUD_new_CTNRO)) ),CONCAT( '|PGCOD=', RTRIM(COALESCE(AUD_old_PGCOD,AUD_new_PGCOD)) ) ),CONCAT( '|UBUSER=', RTRIM(COALESCE(AUD_old_UBUSER,AUD_new_UBUSER)) ) ),'|' ),AUD_FST056_UBT,AUD_FST056_UBU,AUD_FST056_UBM,AUD_FST056_UBP,AUD_FST056_UBR,AUD_FST056_UAs,AUD_FST056_UBC,AUD_FST056_UBA,AUD_FST056_UBS, '', 'BANTOTAL', '', '', '',AUD_FST056_UT FROM BANTPROD.AUD_FST056_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST056_UOn AND AUDMstId = AUD_FST056_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST056_UOn,AUD_FST056_GuidKey,'CTNRO',AUD_old_CTNRO,AUD_new_CTNRO FROM BANTPROD.AUD_FST056_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST056_UOn AND AUDMstId = AUD_FST056_GuidKey AND AUDMstFld = 'CTNRO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST056_UOn,AUD_FST056_GuidKey,'PGCOD',AUD_old_PGCOD,AUD_new_PGCOD FROM BANTPROD.AUD_FST056_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST056_UOn AND AUDMstId = AUD_FST056_GuidKey AND AUDMstFld = 'PGCOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST056_UOn,AUD_FST056_GuidKey,'UBUSER',AUD_old_UBUSER,AUD_new_UBUSER FROM BANTPROD.AUD_FST056_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST056_UOn AND AUDMstId = AUD_FST056_GuidKey AND AUDMstFld = 'UBUSER'); DELETE FROM AUD_FST056_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST056_UOn AND AUDMstId = AUD_FST056_GuidKey);  COMMIT ;END IF;END;
