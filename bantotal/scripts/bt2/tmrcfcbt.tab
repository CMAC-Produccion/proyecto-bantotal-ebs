create table TMRCFCBT
(
  c_ctafca VARCHAR2(20),
  n_rubbtt NUMBER(13),
  c_indage VARCHAR2(1)
)
;
comment on table TMRCFCBT
  is 'Equivalencia Rubros contables Finca Bantotal - Rubros no inventariables';
comment on column TMRCFCBT.c_ctafca
  is 'Cta contable Finca';
comment on column TMRCFCBT.n_rubbtt
  is 'Rubro Bantotal';
comment on column TMRCFCBT.c_indage
  is 'Indica Rubro finca contiene Agencia';

