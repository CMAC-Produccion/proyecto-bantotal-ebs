CREATE OR REPLACE PROCEDURE AUD_C_SNG058_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_SNG058_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_SNG058_UOn,AUD_SNG058_GuidKey,'SNG058',CONCAT( CONCAT( CONCAT( CONCAT( '|SNG055EMP=', RTRIM(COALESCE(AUD_old_SNG055EMP,AUD_new_SNG055EMP)) ),CONCAT( '|SNG058REG=', RTRIM(COALESCE(AUD_old_SNG058REG,AUD_new_SNG058REG)) ) ),CONCAT( '|SNG058RSU=', RTRIM(COALESCE(AUD_old_SNG058RSU,AUD_new_SNG058RSU)) ) ),'|' ),AUD_SNG058_UBT,AUD_SNG058_UBU,AUD_SNG058_UBM,AUD_SNG058_UBP,AUD_SNG058_UBR,AUD_SNG058_UAs,AUD_SNG058_UBC,AUD_SNG058_UBA,AUD_SNG058_UBS, '', 'BANTOTAL', '', '', '',AUD_SNG058_UT FROM BANTPROD.AUD_SNG058_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG058_UOn AND AUDMstId = AUD_SNG058_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG058_UOn,AUD_SNG058_GuidKey,'SNG055EMP',AUD_old_SNG055EMP,AUD_new_SNG055EMP FROM BANTPROD.AUD_SNG058_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG058_UOn AND AUDMstId = AUD_SNG058_GuidKey AND AUDMstFld = 'SNG055EMP'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG058_UOn,AUD_SNG058_GuidKey,'SNG058REG',AUD_old_SNG058REG,AUD_new_SNG058REG FROM BANTPROD.AUD_SNG058_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG058_UOn AND AUDMstId = AUD_SNG058_GuidKey AND AUDMstFld = 'SNG058REG'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG058_UOn,AUD_SNG058_GuidKey,'SNG058RSU',AUD_old_SNG058RSU,AUD_new_SNG058RSU FROM BANTPROD.AUD_SNG058_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG058_UOn AND AUDMstId = AUD_SNG058_GuidKey AND AUDMstFld = 'SNG058RSU'); DELETE FROM AUD_SNG058_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG058_UOn AND AUDMstId = AUD_SNG058_GuidKey);  COMMIT ;END IF;END;
