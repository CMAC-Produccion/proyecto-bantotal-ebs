CREATE OR REPLACE TRIGGER T_AUD_FPP030_AUD_D  AFTER DELETE ON FPP030  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FPP030_AUDIT (AUD_old_PP010PRD,AUD_old_PP016COD,AUD_old_PP016TIPO,AUD_old_PP017PAR,AUD_old_PP030CAR1,AUD_old_PP030CAR2,AUD_old_PP030CAR3,AUD_old_PP030DSC,AUD_old_PP030EMP,AUD_old_PP030FEC1,AUD_old_PP030FEC2,AUD_old_PP030FEC3,AUD_old_PP030IMP1,AUD_old_PP030IMP2,AUD_old_PP030IMP3,AUD_old_PP030MDA,AUD_old_PP030MOD,AUD_old_PP030MODI,AUD_old_PP030NUM1,AUD_old_PP030NUM2,AUD_old_PP030NUM3,AUD_old_PP030ORD,AUD_old_PP030PAP,AUD_old_PP030TOP, AUD_FPP030_GuidKey ,AUD_FPP030_UOn,AUD_FPP030_UT ,AUD_FPP030_UBR ,AUD_FPP030_UBC ,AUD_FPP030_UBT ,AUD_FPP030_UBU ,AUD_FPP030_UBA ,AUD_FPP030_UBS ,AUD_FPP030_UBP ,AUD_FPP030_UBM ,AUD_FPP030_UAs )      VALUES (:old.PP010PRD,:old.PP016COD,:old.PP016TIPO,:old.PP017PAR,:old.PP030CAR1,:old.PP030CAR2,:old.PP030CAR3,:old.PP030DSC,:old.PP030EMP,:old.PP030FEC1,:old.PP030FEC2,:old.PP030FEC3,:old.PP030IMP1,:old.PP030IMP2,:old.PP030IMP3,:old.PP030MDA,:old.PP030MOD,:old.PP030MODI,:old.PP030NUM1,:old.PP030NUM2,:old.PP030NUM3,:old.PP030ORD,:old.PP030PAP,:old.PP030TOP, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'D' ); END T_AUD_FPP030_AUD_D;
