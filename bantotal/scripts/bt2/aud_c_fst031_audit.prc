CREATE OR REPLACE PROCEDURE AUD_C_FST031_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_FST031_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_FST031_UOn,AUD_FST031_GuidKey,'FST031',CONCAT( CONCAT( CONCAT( '|GRNRO=', RTRIM(COALESCE(AUD_old_GRNRO,AUD_new_GRNRO)) ),CONCAT( '|TGCOD=', RTRIM(COALESCE(AUD_old_TGCOD,AUD_new_TGCOD)) ) ),'|' ),AUD_FST031_UBT,AUD_FST031_UBU,AUD_FST031_UBM,AUD_FST031_UBP,AUD_FST031_UBR,AUD_FST031_UAs,AUD_FST031_UBC,AUD_FST031_UBA,AUD_FST031_UBS, '', 'BANTOTAL', '', '', '',AUD_FST031_UT FROM BANTPROD.AUD_FST031_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST031_UOn AND AUDMstId = AUD_FST031_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST031_UOn,AUD_FST031_GuidKey,'GRNOM',AUD_old_GRNOM,AUD_new_GRNOM FROM BANTPROD.AUD_FST031_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST031_UOn AND AUDMstId = AUD_FST031_GuidKey AND AUDMstFld = 'GRNOM'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST031_UOn,AUD_FST031_GuidKey,'GRNRO',AUD_old_GRNRO,AUD_new_GRNRO FROM BANTPROD.AUD_FST031_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST031_UOn AND AUDMstId = AUD_FST031_GuidKey AND AUDMstFld = 'GRNRO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST031_UOn,AUD_FST031_GuidKey,'TGCOD',AUD_old_TGCOD,AUD_new_TGCOD FROM BANTPROD.AUD_FST031_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST031_UOn AND AUDMstId = AUD_FST031_GuidKey AND AUDMstFld = 'TGCOD'); DELETE FROM AUD_FST031_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST031_UOn AND AUDMstId = AUD_FST031_GuidKey);  COMMIT ;END IF;END;
