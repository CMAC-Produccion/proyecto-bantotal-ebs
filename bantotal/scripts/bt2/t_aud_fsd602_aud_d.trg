CREATE OR REPLACE TRIGGER T_AUD_FSD602_AUD_D  AFTER DELETE ON FSD602  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FSD602_AUDIT (AUD_old_D602CD,AUD_old_D602CO,AUD_old_D602FC,AUD_old_D602MO,AUD_old_D602OR,AUD_old_D602RE,AUD_old_D602SB,AUD_old_D602SU,AUD_old_D602TR,AUD_old_PGCOD,AUD_old_PP1CAP,AUD_old_PP1FECH,AUD_old_PP1ICAP,AUD_old_PP1IINT,AUD_old_PP1IINTM,AUD_old_PP1INT,AUD_old_PP1INTM,AUD_old_PP1INTMEX,AUD_old_PP1INTMMEX,AUD_old_PP1NUMP,AUD_old_PP1SALADE,AUD_old_PP1SALADEM,AUD_old_PP1SALCAP,AUD_old_PP1SALINT,AUD_old_PP1SALINTM,AUD_old_PP1SALMOR,AUD_old_PP1SALMORM,AUD_old_PP1STAT,AUD_old_PPCTA,AUD_old_PPFPAG,AUD_old_PPMDA,AUD_old_PPMOD,AUD_old_PPOPER,AUD_old_PPPAP,AUD_old_PPSBOP,AUD_old_PPSUC,AUD_old_PPTIPO,AUD_old_PPTOPE, AUD_FSD602_GuidKey ,AUD_FSD602_UOn,AUD_FSD602_UT ,AUD_FSD602_UBR ,AUD_FSD602_UBC ,AUD_FSD602_UBT ,AUD_FSD602_UBU ,AUD_FSD602_UBA ,AUD_FSD602_UBS ,AUD_FSD602_UBP ,AUD_FSD602_UBM ,AUD_FSD602_UAs )      VALUES (:old.D602CD,:old.D602CO,:old.D602FC,:old.D602MO,:old.D602OR,:old.D602RE,:old.D602SB,:old.D602SU,:old.D602TR,:old.PGCOD,:old.PP1CAP,:old.PP1FECH,:old.PP1ICAP,:old.PP1IINT,:old.PP1IINTM,:old.PP1INT,:old.PP1INTM,:old.PP1INTMEX,:old.PP1INTMMEX,:old.PP1NUMP,:old.PP1SALADE,:old.PP1SALADEM,:old.PP1SALCAP,:old.PP1SALINT,:old.PP1SALINTM,:old.PP1SALMOR,:old.PP1SALMORM,:old.PP1STAT,:old.PPCTA,:old.PPFPAG,:old.PPMDA,:old.PPMOD,:old.PPOPER,:old.PPPAP,:old.PPSBOP,:old.PPSUC,:old.PPTIPO,:old.PPTOPE, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'D' ); END T_AUD_FSD602_AUD_D;
