AS
SELECT Z0E478THP    PEPAIS,
       Z0E478THT    PETDOC,
       Z0E478THD    PENDOC,
       Z0E478EST,
       Z0E463COD,
       JAQZ205HABIL,
       JAQZ205FEAFI,
       JAQZ205HOAFI,
       JAQZ205FEULT,
       JAQZ205HOULT
  FROM JAQZ205 A
  LEFT JOIN Z0E478 B
    ON A.JAQZ205NUTAR = B.Z0E478NRO;

