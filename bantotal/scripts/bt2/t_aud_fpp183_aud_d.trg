CREATE OR REPLACE TRIGGER T_AUD_FPP183_AUD_D  AFTER DELETE ON FPP183  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FPP183_AUDIT (AUD_old_PP173COD,AUD_old_PP174COD,AUD_old_PP182COD,AUD_old_PP183AUX1,AUD_old_PP183AUX10,AUD_old_PP183AUX11,AUD_old_PP183AUX12,AUD_old_PP183AUX2,AUD_old_PP183AUX3,AUD_old_PP183AUX4,AUD_old_PP183AUX5,AUD_old_PP183AUX6,AUD_old_PP183AUX7,AUD_old_PP183AUX8,AUD_old_PP183AUX9,AUD_old_PP183COD,AUD_old_PP183CON,AUD_old_PP183FCON,AUD_old_PP183FEC,AUD_old_PP183MOD,AUD_old_PP183NREL,AUD_old_PP183ORD,AUD_old_PP183PGCOD,AUD_old_PP183SBO,AUD_old_PP183SUC,AUD_old_PP183TRAN, AUD_FPP183_GuidKey ,AUD_FPP183_UOn,AUD_FPP183_UT ,AUD_FPP183_UBR ,AUD_FPP183_UBC ,AUD_FPP183_UBT ,AUD_FPP183_UBU ,AUD_FPP183_UBA ,AUD_FPP183_UBS ,AUD_FPP183_UBP ,AUD_FPP183_UBM ,AUD_FPP183_UAs )      VALUES (:old.PP173COD,:old.PP174COD,:old.PP182COD,:old.PP183AUX1,:old.PP183AUX10,:old.PP183AUX11,:old.PP183AUX12,:old.PP183AUX2,:old.PP183AUX3,:old.PP183AUX4,:old.PP183AUX5,:old.PP183AUX6,:old.PP183AUX7,:old.PP183AUX8,:old.PP183AUX9,:old.PP183COD,:old.PP183CON,:old.PP183FCON,:old.PP183FEC,:old.PP183MOD,:old.PP183NREL,:old.PP183ORD,:old.PP183PGCOD,:old.PP183SBO,:old.PP183SUC,:old.PP183TRAN, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'D' ); END T_AUD_FPP183_AUD_D;
