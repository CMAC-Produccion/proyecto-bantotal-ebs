CREATE OR REPLACE TRIGGER T_AUD_AQPA730_AUD_D  AFTER DELETE ON AQPA730  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_AQPA730_AUDIT (AUD_old_AQPA730ACTION,AUD_old_AQPA730ACTIONBT,AUD_old_AQPA730CHNCAR,AUD_old_AQPA730EVENTO,AUD_old_AQPA730EVENTOBT,AUD_old_AQPA730FECACT,AUD_old_AQPA730FECBT,AUD_old_AQPA730FECCAR,AUD_old_AQPA730FECREG,AUD_old_AQPA730HORACT,AUD_old_AQPA730HORBT,AUD_old_AQPA730HORCAR,AUD_old_AQPA730HORREG,AUD_old_AQPA730ID,AUD_old_AQPA730NUMDOC,AUD_old_AQPA730NUMTAR,AUD_old_AQPA730PAIDOC,AUD_old_AQPA730TARORI,AUD_old_AQPA730TIPDOC,AUD_old_AQPA730TOKEN,AUD_old_AQPA730TOKEXP,AUD_old_AQPA730TOKREFID,AUD_old_AQPA730TOKREQID,AUD_old_AQPA730TOKREQNAM,AUD_old_AQPA730TOKTYP,AUD_old_AQPA730ULTEVE, AUD_AQPA730_GuidKey ,AUD_AQPA730_UOn,AUD_AQPA730_UT ,AUD_AQPA730_UBR ,AUD_AQPA730_UBC ,AUD_AQPA730_UBT ,AUD_AQPA730_UBU ,AUD_AQPA730_UBA ,AUD_AQPA730_UBS ,AUD_AQPA730_UBP ,AUD_AQPA730_UBM ,AUD_AQPA730_UAs )      VALUES (:old.AQPA730ACTION,:old.AQPA730ACTIONBT,:old.AQPA730CHNCAR,:old.AQPA730EVENTO,:old.AQPA730EVENTOBT,:old.AQPA730FECACT,:old.AQPA730FECBT,:old.AQPA730FECCAR,:old.AQPA730FECREG,:old.AQPA730HORACT,:old.AQPA730HORBT,:old.AQPA730HORCAR,:old.AQPA730HORREG,:old.AQPA730ID,:old.AQPA730NUMDOC,:old.AQPA730NUMTAR,:old.AQPA730PAIDOC,:old.AQPA730TARORI,:old.AQPA730TIPDOC,:old.AQPA730TOKEN,:old.AQPA730TOKEXP,:old.AQPA730TOKREFID,:old.AQPA730TOKREQID,:old.AQPA730TOKREQNAM,:old.AQPA730TOKTYP,:old.AQPA730ULTEVE, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'D' ); END T_AUD_AQPA730_AUD_D;
