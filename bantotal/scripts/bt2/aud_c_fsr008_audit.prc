CREATE OR REPLACE PROCEDURE AUD_C_FSR008_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_FSR008_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_FSR008_UOn,AUD_FSR008_GuidKey,'FSR008',CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( '|CTNRO=', RTRIM(COALESCE(AUD_old_CTNRO,AUD_new_CTNRO)) ),CONCAT( '|PENDOC=', RTRIM(COALESCE(AUD_old_PENDOC,AUD_new_PENDOC)) ) ),CONCAT( '|PEPAIS=', RTRIM(COALESCE(AUD_old_PEPAIS,AUD_new_PEPAIS)) ) ),CONCAT( '|PETDOC=', RTRIM(COALESCE(AUD_old_PETDOC,AUD_new_PETDOC)) ) ),CONCAT( '|PGCOD=', RTRIM(COALESCE(AUD_old_PGCOD,AUD_new_PGCOD)) ) ),'|' ),AUD_FSR008_UBT,AUD_FSR008_UBU,AUD_FSR008_UBM,AUD_FSR008_UBP,AUD_FSR008_UBR,AUD_FSR008_UAs,AUD_FSR008_UBC,AUD_FSR008_UBA,AUD_FSR008_UBS, '', 'BANTOTAL', '', '', '',AUD_FSR008_UT FROM BANTPROD.AUD_FSR008_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FSR008_UOn AND AUDMstId = AUD_FSR008_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR008_UOn,AUD_FSR008_GuidKey,'CTNRO',AUD_old_CTNRO,AUD_new_CTNRO FROM BANTPROD.AUD_FSR008_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR008_UOn AND AUDMstId = AUD_FSR008_GuidKey AND AUDMstFld = 'CTNRO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR008_UOn,AUD_FSR008_GuidKey,'CTTFIR',AUD_old_CTTFIR,AUD_new_CTTFIR FROM BANTPROD.AUD_FSR008_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR008_UOn AND AUDMstId = AUD_FSR008_GuidKey AND AUDMstFld = 'CTTFIR'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR008_UOn,AUD_FSR008_GuidKey,'PENDOC',AUD_old_PENDOC,AUD_new_PENDOC FROM BANTPROD.AUD_FSR008_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR008_UOn AND AUDMstId = AUD_FSR008_GuidKey AND AUDMstFld = 'PENDOC'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR008_UOn,AUD_FSR008_GuidKey,'PEPAIS',AUD_old_PEPAIS,AUD_new_PEPAIS FROM BANTPROD.AUD_FSR008_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR008_UOn AND AUDMstId = AUD_FSR008_GuidKey AND AUDMstFld = 'PEPAIS'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR008_UOn,AUD_FSR008_GuidKey,'PETDOC',AUD_old_PETDOC,AUD_new_PETDOC FROM BANTPROD.AUD_FSR008_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR008_UOn AND AUDMstId = AUD_FSR008_GuidKey AND AUDMstFld = 'PETDOC'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR008_UOn,AUD_FSR008_GuidKey,'PGCOD',AUD_old_PGCOD,AUD_new_PGCOD FROM BANTPROD.AUD_FSR008_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR008_UOn AND AUDMstId = AUD_FSR008_GuidKey AND AUDMstFld = 'PGCOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR008_UOn,AUD_FSR008_GuidKey,'TTCOD',AUD_old_TTCOD,AUD_new_TTCOD FROM BANTPROD.AUD_FSR008_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR008_UOn AND AUDMstId = AUD_FSR008_GuidKey AND AUDMstFld = 'TTCOD'); DELETE FROM AUD_FSR008_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FSR008_UOn AND AUDMstId = AUD_FSR008_GuidKey);  COMMIT ;END IF;END;
