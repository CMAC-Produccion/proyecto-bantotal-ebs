CREATE OR REPLACE TRIGGER T_AUD_SNGC60_AUD_I  AFTER INSERT ON SNGC60  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_SNGC60_AUDIT ( AUD_new_SNGC60ACTE,AUD_new_SNGC60AUX1,AUD_new_SNGC60AUX2,AUD_new_SNGC60AUX3,AUD_new_SNGC60AUX4,AUD_new_SNGC60AUX5,AUD_new_SNGC60CORR,AUD_new_SNGC60ERUT,AUD_new_SNGC60FINE,AUD_new_SNGC60FINI,AUD_new_SNGC60NDOC,AUD_new_SNGC60NOME,AUD_new_SNGC60OCUP,AUD_new_SNGC60PAIS,AUD_new_SNGC60RUTE,AUD_new_SNGC60RUTP,AUD_new_SNGC60RZSO,AUD_new_SNGC60TDOC,AUD_new_SNGC60TIPA,AUD_new_SNGC60UBIC,AUD_new_SNGC60VCOD, AUD_SNGC60_GuidKey ,AUD_SNGC60_UOn,AUD_SNGC60_UT ,AUD_SNGC60_UBR ,AUD_SNGC60_UBC ,AUD_SNGC60_UBT ,AUD_SNGC60_UBU ,AUD_SNGC60_UBA ,AUD_SNGC60_UBS ,AUD_SNGC60_UBP ,AUD_SNGC60_UBM ,AUD_SNGC60_UAs  )      VALUES (:new.SNGC60ACTE,:new.SNGC60AUX1,:new.SNGC60AUX2,:new.SNGC60AUX3,:new.SNGC60AUX4,:new.SNGC60AUX5,:new.SNGC60CORR,:new.SNGC60ERUT,:new.SNGC60FINE,:new.SNGC60FINI,:new.SNGC60NDOC,:new.SNGC60NOME,:new.SNGC60OCUP,:new.SNGC60PAIS,:new.SNGC60RUTE,:new.SNGC60RUTP,:new.SNGC60RZSO,:new.SNGC60TDOC,:new.SNGC60TIPA,:new.SNGC60UBIC,:new.SNGC60VCOD, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'I'); END T_AUD_SNGC60_AUD_I;
