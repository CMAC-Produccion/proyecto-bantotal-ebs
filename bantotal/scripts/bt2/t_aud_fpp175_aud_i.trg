CREATE OR REPLACE TRIGGER T_AUD_FPP175_AUD_I  AFTER INSERT ON FPP175  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FPP175_AUDIT ( AUD_new_PP173COD,AUD_new_PP174COD,AUD_new_PP175AU1,AUD_new_PP175AU10,AUD_new_PP175AU11,AUD_new_PP175AU12,AUD_new_PP175AU2,AUD_new_PP175AU3,AUD_new_PP175AU4,AUD_new_PP175AU5,AUD_new_PP175AU6,AUD_new_PP175AU7,AUD_new_PP175AU8,AUD_new_PP175AU9,AUD_new_PP175CORR,AUD_new_PP175CTA,AUD_new_PP175MDA,AUD_new_PP175MOD,AUD_new_PP175OPER,AUD_new_PP175PAP,AUD_new_PP175PGCOD,AUD_new_PP175SBOP,AUD_new_PP175SUC,AUD_new_PP175TCO,AUD_new_PP175TFCO,AUD_new_PP175TMOD,AUD_new_PP175TNREL,AUD_new_PP175TOPE,AUD_new_PP175TORD,AUD_new_PP175TPGC,AUD_new_PP175TSBO,AUD_new_PP175TSUC,AUD_new_PP175TTRAN, AUD_FPP175_GuidKey ,AUD_FPP175_UOn,AUD_FPP175_UT ,AUD_FPP175_UBR ,AUD_FPP175_UBC ,AUD_FPP175_UBT ,AUD_FPP175_UBU ,AUD_FPP175_UBA ,AUD_FPP175_UBS ,AUD_FPP175_UBP ,AUD_FPP175_UBM ,AUD_FPP175_UAs  )      VALUES (:new.PP173COD,:new.PP174COD,:new.PP175AU1,:new.PP175AU10,:new.PP175AU11,:new.PP175AU12,:new.PP175AU2,:new.PP175AU3,:new.PP175AU4,:new.PP175AU5,:new.PP175AU6,:new.PP175AU7,:new.PP175AU8,:new.PP175AU9,:new.PP175CORR,:new.PP175CTA,:new.PP175MDA,:new.PP175MOD,:new.PP175OPER,:new.PP175PAP,:new.PP175PGCOD,:new.PP175SBOP,:new.PP175SUC,:new.PP175TCO,:new.PP175TFCO,:new.PP175TMOD,:new.PP175TNREL,:new.PP175TOPE,:new.PP175TORD,:new.PP175TPGC,:new.PP175TSBO,:new.PP175TSUC,:new.PP175TTRAN, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'I'); END T_AUD_FPP175_AUD_I;
