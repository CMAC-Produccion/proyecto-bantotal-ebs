CREATE OR REPLACE PROCEDURE AUD_C_FST139_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_FST139_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_FST139_UOn,AUD_FST139_GuidKey,'FST139',CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( '|PGCOD=', RTRIM(COALESCE(AUD_old_PGCOD,AUD_new_PGCOD)) ),CONCAT( '|TRMOD=', RTRIM(COALESCE(AUD_old_TRMOD,AUD_new_TRMOD)) ) ),CONCAT( '|TRNRO=', RTRIM(COALESCE(AUD_old_TRNRO,AUD_new_TRNRO)) ) ),CONCAT( '|TRORD=', RTRIM(COALESCE(AUD_old_TRORD,AUD_new_TRORD)) ) ),CONCAT( '|TRWRKST=', RTRIM(COALESCE(AUD_old_TRWRKST,AUD_new_TRWRKST)) ) ),'|' ),AUD_FST139_UBT,AUD_FST139_UBU,AUD_FST139_UBM,AUD_FST139_UBP,AUD_FST139_UBR,AUD_FST139_UAs,AUD_FST139_UBC,AUD_FST139_UBA,AUD_FST139_UBS, '', 'BANTOTAL', '', '', '',AUD_FST139_UT FROM BANTPROD.AUD_FST139_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST139_UOn AND AUDMstId = AUD_FST139_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST139_UOn,AUD_FST139_GuidKey,'INTSTRF',AUD_old_INTSTRF,AUD_new_INTSTRF FROM BANTPROD.AUD_FST139_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST139_UOn AND AUDMstId = AUD_FST139_GuidKey AND AUDMstFld = 'INTSTRF'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST139_UOn,AUD_FST139_GuidKey,'INTSTRI',AUD_old_INTSTRI,AUD_new_INTSTRI FROM BANTPROD.AUD_FST139_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST139_UOn AND AUDMstId = AUD_FST139_GuidKey AND AUDMstFld = 'INTSTRI'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST139_UOn,AUD_FST139_GuidKey,'PGCOD',AUD_old_PGCOD,AUD_new_PGCOD FROM BANTPROD.AUD_FST139_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST139_UOn AND AUDMstId = AUD_FST139_GuidKey AND AUDMstFld = 'PGCOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST139_UOn,AUD_FST139_GuidKey,'TRMOD',AUD_old_TRMOD,AUD_new_TRMOD FROM BANTPROD.AUD_FST139_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST139_UOn AND AUDMstId = AUD_FST139_GuidKey AND AUDMstFld = 'TRMOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST139_UOn,AUD_FST139_GuidKey,'TRNRO',AUD_old_TRNRO,AUD_new_TRNRO FROM BANTPROD.AUD_FST139_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST139_UOn AND AUDMstId = AUD_FST139_GuidKey AND AUDMstFld = 'TRNRO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST139_UOn,AUD_FST139_GuidKey,'TRORD',AUD_old_TRORD,AUD_new_TRORD FROM BANTPROD.AUD_FST139_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST139_UOn AND AUDMstId = AUD_FST139_GuidKey AND AUDMstFld = 'TRORD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST139_UOn,AUD_FST139_GuidKey,'TRWRKST',AUD_old_TRWRKST,AUD_new_TRWRKST FROM BANTPROD.AUD_FST139_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST139_UOn AND AUDMstId = AUD_FST139_GuidKey AND AUDMstFld = 'TRWRKST'); DELETE FROM AUD_FST139_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST139_UOn AND AUDMstId = AUD_FST139_GuidKey);  COMMIT ;END IF;END;
