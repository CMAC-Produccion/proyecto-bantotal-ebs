CREATE OR REPLACE PROCEDURE AUD_C_SNG401_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_SNG401_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_SNG401_UOn,AUD_SNG401_GuidKey,'SNG401',CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( '|SNG400COD=', RTRIM(COALESCE(AUD_old_SNG400COD,AUD_new_SNG400COD)) ),CONCAT( '|SNG400EVTO=', RTRIM(COALESCE(AUD_old_SNG400EVTO,AUD_new_SNG400EVTO)) ) ),CONCAT( '|SNG401MOD=', RTRIM(COALESCE(AUD_old_SNG401MOD,AUD_new_SNG401MOD)) ) ),CONCAT( '|SNG401TOP=', RTRIM(COALESCE(AUD_old_SNG401TOP,AUD_new_SNG401TOP)) ) ),'|' ),AUD_SNG401_UBT,AUD_SNG401_UBU,AUD_SNG401_UBM,AUD_SNG401_UBP,AUD_SNG401_UBR,AUD_SNG401_UAs,AUD_SNG401_UBC,AUD_SNG401_UBA,AUD_SNG401_UBS, '', 'BANTOTAL', '', '', '',AUD_SNG401_UT FROM BANTPROD.AUD_SNG401_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG401_UOn AND AUDMstId = AUD_SNG401_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG401_UOn,AUD_SNG401_GuidKey,'SNG400COD',AUD_old_SNG400COD,AUD_new_SNG400COD FROM BANTPROD.AUD_SNG401_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG401_UOn AND AUDMstId = AUD_SNG401_GuidKey AND AUDMstFld = 'SNG400COD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG401_UOn,AUD_SNG401_GuidKey,'SNG400EVTO',AUD_old_SNG400EVTO,AUD_new_SNG400EVTO FROM BANTPROD.AUD_SNG401_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG401_UOn AND AUDMstId = AUD_SNG401_GuidKey AND AUDMstFld = 'SNG400EVTO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG401_UOn,AUD_SNG401_GuidKey,'SNG401MOD',AUD_old_SNG401MOD,AUD_new_SNG401MOD FROM BANTPROD.AUD_SNG401_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG401_UOn AND AUDMstId = AUD_SNG401_GuidKey AND AUDMstFld = 'SNG401MOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG401_UOn,AUD_SNG401_GuidKey,'SNG401TOP',AUD_old_SNG401TOP,AUD_new_SNG401TOP FROM BANTPROD.AUD_SNG401_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG401_UOn AND AUDMstId = AUD_SNG401_GuidKey AND AUDMstFld = 'SNG401TOP'); DELETE FROM AUD_SNG401_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG401_UOn AND AUDMstId = AUD_SNG401_GuidKey);  COMMIT ;END IF;END;
