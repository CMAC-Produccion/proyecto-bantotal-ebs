CREATE OR REPLACE TRIGGER T_AUD_FSR101_AUD_U  AFTER UPDATE ON FSR101  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;     INSERT INTO BANTPROD.AUD_FSR101_AUDIT (AUD_new_PBCOD,AUD_new_PBD1,AUD_new_PBD2,AUD_new_PBD3,AUD_new_PBD4,AUD_new_PBD5,AUD_new_PBD6,AUD_new_PBD7,AUD_new_PBD8,AUD_new_PBH1,AUD_new_PBH2,AUD_new_PBH3,AUD_new_PBH4,AUD_new_PBH5,AUD_new_PBH6,AUD_new_PBH7,AUD_new_PBH8,AUD_new_PBIMPR,AUD_new_PBNSEC,AUD_new_PBPATH,AUD_new_PBTHREAD, AUD_old_PBCOD,AUD_old_PBD1,AUD_old_PBD2,AUD_old_PBD3,AUD_old_PBD4,AUD_old_PBD5,AUD_old_PBD6,AUD_old_PBD7,AUD_old_PBD8,AUD_old_PBH1,AUD_old_PBH2,AUD_old_PBH3,AUD_old_PBH4,AUD_old_PBH5,AUD_old_PBH6,AUD_old_PBH7,AUD_old_PBH8,AUD_old_PBIMPR,AUD_old_PBNSEC,AUD_old_PBPATH,AUD_old_PBTHREAD, AUD_FSR101_GuidKey ,AUD_FSR101_UOn,AUD_FSR101_UT ,AUD_FSR101_UBR ,AUD_FSR101_UBC ,AUD_FSR101_UBT ,AUD_FSR101_UBU ,AUD_FSR101_UBA ,AUD_FSR101_UBS ,AUD_FSR101_UBP ,AUD_FSR101_UBM ,AUD_FSR101_UAs )     VALUES (:new.PBCOD,:new.PBD1,:new.PBD2,:new.PBD3,:new.PBD4,:new.PBD5,:new.PBD6,:new.PBD7,:new.PBD8,:new.PBH1,:new.PBH2,:new.PBH3,:new.PBH4,:new.PBH5,:new.PBH6,:new.PBH7,:new.PBH8,:new.PBIMPR,:new.PBNSEC,:new.PBPATH,:new.PBTHREAD,  :old.PBCOD,:old.PBD1,:old.PBD2,:old.PBD3,:old.PBD4,:old.PBD5,:old.PBD6,:old.PBD7,:old.PBD8,:old.PBH1,:old.PBH2,:old.PBH3,:old.PBH4,:old.PBH5,:old.PBH6,:old.PBH7,:old.PBH8,:old.PBIMPR,:old.PBNSEC,:old.PBPATH,:old.PBTHREAD,  SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'U' ); END T_AUD_FSR101_AUD_U;
