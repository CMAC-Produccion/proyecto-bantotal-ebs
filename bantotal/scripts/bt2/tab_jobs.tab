create table TAB_JOBS
(
  c_codage VARCHAR2(10),
  c_detjob VARCHAR2(500),
  d_fecini DATE,
  d_fecfin DATE,
  c_inderr VARCHAR2(1),
  c_deserr VARCHAR2(400),
  c_codpro VARCHAR2(3),
  n_numer1 NUMBER,
  n_numer2 NUMBER,
  n_numer3 NUMBER,
  c_dato1  VARCHAR2(200),
  c_dato2  VARCHAR2(200)
)
;
comment on table TAB_JOBS
  is 'Tabla log de errores en procesos con jobs';
create index IDX$$_00840001 on TAB_JOBS (C_DETJOB, N_NUMER1);

