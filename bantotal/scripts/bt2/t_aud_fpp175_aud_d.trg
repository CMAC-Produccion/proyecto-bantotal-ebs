CREATE OR REPLACE TRIGGER T_AUD_FPP175_AUD_D  AFTER DELETE ON FPP175  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FPP175_AUDIT (AUD_old_PP173COD,AUD_old_PP174COD,AUD_old_PP175AU1,AUD_old_PP175AU10,AUD_old_PP175AU11,AUD_old_PP175AU12,AUD_old_PP175AU2,AUD_old_PP175AU3,AUD_old_PP175AU4,AUD_old_PP175AU5,AUD_old_PP175AU6,AUD_old_PP175AU7,AUD_old_PP175AU8,AUD_old_PP175AU9,AUD_old_PP175CORR,AUD_old_PP175CTA,AUD_old_PP175MDA,AUD_old_PP175MOD,AUD_old_PP175OPER,AUD_old_PP175PAP,AUD_old_PP175PGCOD,AUD_old_PP175SBOP,AUD_old_PP175SUC,AUD_old_PP175TCO,AUD_old_PP175TFCO,AUD_old_PP175TMOD,AUD_old_PP175TNREL,AUD_old_PP175TOPE,AUD_old_PP175TORD,AUD_old_PP175TPGC,AUD_old_PP175TSBO,AUD_old_PP175TSUC,AUD_old_PP175TTRAN, AUD_FPP175_GuidKey ,AUD_FPP175_UOn,AUD_FPP175_UT ,AUD_FPP175_UBR ,AUD_FPP175_UBC ,AUD_FPP175_UBT ,AUD_FPP175_UBU ,AUD_FPP175_UBA ,AUD_FPP175_UBS ,AUD_FPP175_UBP ,AUD_FPP175_UBM ,AUD_FPP175_UAs )      VALUES (:old.PP173COD,:old.PP174COD,:old.PP175AU1,:old.PP175AU10,:old.PP175AU11,:old.PP175AU12,:old.PP175AU2,:old.PP175AU3,:old.PP175AU4,:old.PP175AU5,:old.PP175AU6,:old.PP175AU7,:old.PP175AU8,:old.PP175AU9,:old.PP175CORR,:old.PP175CTA,:old.PP175MDA,:old.PP175MOD,:old.PP175OPER,:old.PP175PAP,:old.PP175PGCOD,:old.PP175SBOP,:old.PP175SUC,:old.PP175TCO,:old.PP175TFCO,:old.PP175TMOD,:old.PP175TNREL,:old.PP175TOPE,:old.PP175TORD,:old.PP175TPGC,:old.PP175TSBO,:old.PP175TSUC,:old.PP175TTRAN, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'D' ); END T_AUD_FPP175_AUD_D;
