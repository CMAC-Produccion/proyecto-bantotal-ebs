CREATE OR REPLACE PROCEDURE AUD_C_SNG405_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_SNG405_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_SNG405_UOn,AUD_SNG405_GuidKey,'SNG405',CONCAT( CONCAT( CONCAT( CONCAT( '|SNG400COD=', RTRIM(COALESCE(AUD_old_SNG400COD,AUD_new_SNG400COD)) ),CONCAT( '|SNG400EVTO=', RTRIM(COALESCE(AUD_old_SNG400EVTO,AUD_new_SNG400EVTO)) ) ),CONCAT( '|SNG405SG=', RTRIM(COALESCE(AUD_old_SNG405SG,AUD_new_SNG405SG)) ) ),'|' ),AUD_SNG405_UBT,AUD_SNG405_UBU,AUD_SNG405_UBM,AUD_SNG405_UBP,AUD_SNG405_UBR,AUD_SNG405_UAs,AUD_SNG405_UBC,AUD_SNG405_UBA,AUD_SNG405_UBS, '', 'BANTOTAL', '', '', '',AUD_SNG405_UT FROM BANTPROD.AUD_SNG405_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG405_UOn AND AUDMstId = AUD_SNG405_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG405_UOn,AUD_SNG405_GuidKey,'SNG400COD',AUD_old_SNG400COD,AUD_new_SNG400COD FROM BANTPROD.AUD_SNG405_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG405_UOn AND AUDMstId = AUD_SNG405_GuidKey AND AUDMstFld = 'SNG400COD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG405_UOn,AUD_SNG405_GuidKey,'SNG400EVTO',AUD_old_SNG400EVTO,AUD_new_SNG400EVTO FROM BANTPROD.AUD_SNG405_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG405_UOn AND AUDMstId = AUD_SNG405_GuidKey AND AUDMstFld = 'SNG400EVTO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG405_UOn,AUD_SNG405_GuidKey,'SNG405SG',AUD_old_SNG405SG,AUD_new_SNG405SG FROM BANTPROD.AUD_SNG405_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG405_UOn AND AUDMstId = AUD_SNG405_GuidKey AND AUDMstFld = 'SNG405SG'); DELETE FROM AUD_SNG405_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG405_UOn AND AUDMstId = AUD_SNG405_GuidKey);  COMMIT ;END IF;END;
