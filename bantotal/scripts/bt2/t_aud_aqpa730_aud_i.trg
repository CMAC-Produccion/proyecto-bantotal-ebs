CREATE OR REPLACE TRIGGER T_AUD_AQPA730_AUD_I  AFTER INSERT ON AQPA730  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_AQPA730_AUDIT ( AUD_new_AQPA730ACTION,AUD_new_AQPA730ACTIONBT,AUD_new_AQPA730CHNCAR,AUD_new_AQPA730EVENTO,AUD_new_AQPA730EVENTOBT,AUD_new_AQPA730FECACT,AUD_new_AQPA730FECBT,AUD_new_AQPA730FECCAR,AUD_new_AQPA730FECREG,AUD_new_AQPA730HORACT,AUD_new_AQPA730HORBT,AUD_new_AQPA730HORCAR,AUD_new_AQPA730HORREG,AUD_new_AQPA730ID,AUD_new_AQPA730NUMDOC,AUD_new_AQPA730NUMTAR,AUD_new_AQPA730PAIDOC,AUD_new_AQPA730TARORI,AUD_new_AQPA730TIPDOC,AUD_new_AQPA730TOKEN,AUD_new_AQPA730TOKEXP,AUD_new_AQPA730TOKREFID,AUD_new_AQPA730TOKREQID,AUD_new_AQPA730TOKREQNAM,AUD_new_AQPA730TOKTYP,AUD_new_AQPA730ULTEVE, AUD_AQPA730_GuidKey ,AUD_AQPA730_UOn,AUD_AQPA730_UT ,AUD_AQPA730_UBR ,AUD_AQPA730_UBC ,AUD_AQPA730_UBT ,AUD_AQPA730_UBU ,AUD_AQPA730_UBA ,AUD_AQPA730_UBS ,AUD_AQPA730_UBP ,AUD_AQPA730_UBM ,AUD_AQPA730_UAs  )      VALUES (:new.AQPA730ACTION,:new.AQPA730ACTIONBT,:new.AQPA730CHNCAR,:new.AQPA730EVENTO,:new.AQPA730EVENTOBT,:new.AQPA730FECACT,:new.AQPA730FECBT,:new.AQPA730FECCAR,:new.AQPA730FECREG,:new.AQPA730HORACT,:new.AQPA730HORBT,:new.AQPA730HORCAR,:new.AQPA730HORREG,:new.AQPA730ID,:new.AQPA730NUMDOC,:new.AQPA730NUMTAR,:new.AQPA730PAIDOC,:new.AQPA730TARORI,:new.AQPA730TIPDOC,:new.AQPA730TOKEN,:new.AQPA730TOKEXP,:new.AQPA730TOKREFID,:new.AQPA730TOKREQID,:new.AQPA730TOKREQNAM,:new.AQPA730TOKTYP,:new.AQPA730ULTEVE, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'I'); END T_AUD_AQPA730_AUD_I;
