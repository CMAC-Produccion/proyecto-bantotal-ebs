CREATE OR REPLACE PROCEDURE AUD_C_SNG406_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_SNG406_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_SNG406_UOn,AUD_SNG406_GuidKey,'SNG406',CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( '|SNG400COD=', RTRIM(COALESCE(AUD_old_SNG400COD,AUD_new_SNG400COD)) ),CONCAT( '|SNG400EVTO=', RTRIM(COALESCE(AUD_old_SNG400EVTO,AUD_new_SNG400EVTO)) ) ),CONCAT( '|SNG402CR=', RTRIM(COALESCE(AUD_old_SNG402CR,AUD_new_SNG402CR)) ) ),CONCAT( '|SNG406PRG=', RTRIM(COALESCE(AUD_old_SNG406PRG,AUD_new_SNG406PRG)) ) ),'|' ),AUD_SNG406_UBT,AUD_SNG406_UBU,AUD_SNG406_UBM,AUD_SNG406_UBP,AUD_SNG406_UBR,AUD_SNG406_UAs,AUD_SNG406_UBC,AUD_SNG406_UBA,AUD_SNG406_UBS, '', 'BANTOTAL', '', '', '',AUD_SNG406_UT FROM BANTPROD.AUD_SNG406_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG406_UOn AND AUDMstId = AUD_SNG406_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG406_UOn,AUD_SNG406_GuidKey,'SNG400COD',AUD_old_SNG400COD,AUD_new_SNG400COD FROM BANTPROD.AUD_SNG406_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG406_UOn AND AUDMstId = AUD_SNG406_GuidKey AND AUDMstFld = 'SNG400COD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG406_UOn,AUD_SNG406_GuidKey,'SNG400EVTO',AUD_old_SNG400EVTO,AUD_new_SNG400EVTO FROM BANTPROD.AUD_SNG406_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG406_UOn AND AUDMstId = AUD_SNG406_GuidKey AND AUDMstFld = 'SNG400EVTO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG406_UOn,AUD_SNG406_GuidKey,'SNG402CR',AUD_old_SNG402CR,AUD_new_SNG402CR FROM BANTPROD.AUD_SNG406_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG406_UOn AND AUDMstId = AUD_SNG406_GuidKey AND AUDMstFld = 'SNG402CR'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG406_UOn,AUD_SNG406_GuidKey,'SNG406MOD',AUD_old_SNG406MOD,AUD_new_SNG406MOD FROM BANTPROD.AUD_SNG406_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG406_UOn AND AUDMstId = AUD_SNG406_GuidKey AND AUDMstFld = 'SNG406MOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG406_UOn,AUD_SNG406_GuidKey,'SNG406ORD',AUD_old_SNG406ORD,AUD_new_SNG406ORD FROM BANTPROD.AUD_SNG406_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG406_UOn AND AUDMstId = AUD_SNG406_GuidKey AND AUDMstFld = 'SNG406ORD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG406_UOn,AUD_SNG406_GuidKey,'SNG406PRG',AUD_old_SNG406PRG,AUD_new_SNG406PRG FROM BANTPROD.AUD_SNG406_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG406_UOn AND AUDMstId = AUD_SNG406_GuidKey AND AUDMstFld = 'SNG406PRG'); DELETE FROM AUD_SNG406_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG406_UOn AND AUDMstId = AUD_SNG406_GuidKey);  COMMIT ;END IF;END;
