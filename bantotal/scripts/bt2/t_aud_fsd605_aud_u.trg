CREATE OR REPLACE TRIGGER T_AUD_FSD605_AUD_U  AFTER UPDATE ON FSD605  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;     INSERT INTO BANTPROD.AUD_FSD605_AUDIT (AUD_new_RDCLTCOD,AUD_new_RDCOD,AUD_new_RDCTA,AUD_new_RDMDA,AUD_new_RDMOD,AUD_new_RDOPER,AUD_new_RDPAP,AUD_new_RDPLUS,AUD_new_RDPZO,AUD_new_RDSBOP,AUD_new_RDSUC,AUD_new_RDTASA,AUD_new_RDTOPE,AUD_new_RDTTAS, AUD_old_RDCLTCOD,AUD_old_RDCOD,AUD_old_RDCTA,AUD_old_RDMDA,AUD_old_RDMOD,AUD_old_RDOPER,AUD_old_RDPAP,AUD_old_RDPLUS,AUD_old_RDPZO,AUD_old_RDSBOP,AUD_old_RDSUC,AUD_old_RDTASA,AUD_old_RDTOPE,AUD_old_RDTTAS, AUD_FSD605_GuidKey ,AUD_FSD605_UOn,AUD_FSD605_UT ,AUD_FSD605_UBR ,AUD_FSD605_UBC ,AUD_FSD605_UBT ,AUD_FSD605_UBU ,AUD_FSD605_UBA ,AUD_FSD605_UBS ,AUD_FSD605_UBP ,AUD_FSD605_UBM ,AUD_FSD605_UAs )     VALUES (:new.RDCLTCOD,:new.RDCOD,:new.RDCTA,:new.RDMDA,:new.RDMOD,:new.RDOPER,:new.RDPAP,:new.RDPLUS,:new.RDPZO,:new.RDSBOP,:new.RDSUC,:new.RDTASA,:new.RDTOPE,:new.RDTTAS,  :old.RDCLTCOD,:old.RDCOD,:old.RDCTA,:old.RDMDA,:old.RDMOD,:old.RDOPER,:old.RDPAP,:old.RDPLUS,:old.RDPZO,:old.RDSBOP,:old.RDSUC,:old.RDTASA,:old.RDTOPE,:old.RDTTAS,  SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'U' ); END T_AUD_FSD605_AUD_U;
