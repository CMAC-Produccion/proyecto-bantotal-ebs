CREATE OR REPLACE TRIGGER T_AUD_FPP028_AUD_I  AFTER INSERT ON FPP028  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FPP028_AUDIT ( AUD_new_PP010PRD,AUD_new_PP017PAR,AUD_new_PP028AUX1,AUD_new_PP028AUX3,AUD_new_PP028AUX4,AUD_new_PP028AUX5,AUD_new_PP028CAR1,AUD_new_PP028CAR2,AUD_new_PP028CAR3,AUD_new_PP028DEFC,AUD_new_PP028DEFD,AUD_new_PP028DEFN,AUD_new_PP028DEFV,AUD_new_PP028EMP,AUD_new_PP028MAXN,AUD_new_PP028MDA,AUD_new_PP028MINN,AUD_new_PP028MOD,AUD_new_PP028MOD1,AUD_new_PP028MOD2,AUD_new_PP028MOD3,AUD_new_PP028PAP,AUD_new_PP028TIPOS,AUD_new_PP028TOP,AUD_new_PP028VIS1,AUD_new_PP028VIS2,AUD_new_PP028VIS3,AUD_new_PP033AUX2, AUD_FPP028_GuidKey ,AUD_FPP028_UOn,AUD_FPP028_UT ,AUD_FPP028_UBR ,AUD_FPP028_UBC ,AUD_FPP028_UBT ,AUD_FPP028_UBU ,AUD_FPP028_UBA ,AUD_FPP028_UBS ,AUD_FPP028_UBP ,AUD_FPP028_UBM ,AUD_FPP028_UAs  )      VALUES (:new.PP010PRD,:new.PP017PAR,:new.PP028AUX1,:new.PP028AUX3,:new.PP028AUX4,:new.PP028AUX5,:new.PP028CAR1,:new.PP028CAR2,:new.PP028CAR3,:new.PP028DEFC,:new.PP028DEFD,:new.PP028DEFN,:new.PP028DEFV,:new.PP028EMP,:new.PP028MAXN,:new.PP028MDA,:new.PP028MINN,:new.PP028MOD,:new.PP028MOD1,:new.PP028MOD2,:new.PP028MOD3,:new.PP028PAP,:new.PP028TIPOS,:new.PP028TOP,:new.PP028VIS1,:new.PP028VIS2,:new.PP028VIS3,:new.PP033AUX2, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'I'); END T_AUD_FPP028_AUD_I;
