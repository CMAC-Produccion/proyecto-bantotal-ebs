create table BIUSRGST
(
  cod NUMBER,
  usr VARCHAR2(4000),
  tip VARCHAR2(4000),
  des VARCHAR2(4000)
)
;
comment on table BIUSRGST
  is 'Tabla para registro de gerencias regionales y gerentes administrativos';
comment on column BIUSRGST.cod
  is 'codigo de registro';
comment on column BIUSRGST.usr
  is 'codigo de usuario';
comment on column BIUSRGST.tip
  is 'tipo (gerencia regional o gerencia administrativa)';
comment on column BIUSRGST.des
  is 'Region o gerencia';

