CREATE OR REPLACE TRIGGER T_AUD_FSD611_AUD_I  AFTER INSERT ON FSD611  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FSD611_AUDIT ( AUD_new_PGCOD,AUD_new_PPCTA,AUD_new_PPEXTE,AUD_new_PPFPAG,AUD_new_PPIMP1,AUD_new_PPIMP10,AUD_new_PPIMP11,AUD_new_PPIMP12,AUD_new_PPIMP13,AUD_new_PPIMP14,AUD_new_PPIMP15,AUD_new_PPIMP16,AUD_new_PPIMP17,AUD_new_PPIMP18,AUD_new_PPIMP19,AUD_new_PPIMP2,AUD_new_PPIMP20,AUD_new_PPIMP3,AUD_new_PPIMP4,AUD_new_PPIMP5,AUD_new_PPIMP6,AUD_new_PPIMP7,AUD_new_PPIMP8,AUD_new_PPIMP9,AUD_new_PPMDA,AUD_new_PPMOD,AUD_new_PPOPER,AUD_new_PPPAP,AUD_new_PPSBOP,AUD_new_PPSUC,AUD_new_PPTIPO,AUD_new_PPTOPE, AUD_FSD611_GuidKey ,AUD_FSD611_UOn,AUD_FSD611_UT ,AUD_FSD611_UBR ,AUD_FSD611_UBC ,AUD_FSD611_UBT ,AUD_FSD611_UBU ,AUD_FSD611_UBA ,AUD_FSD611_UBS ,AUD_FSD611_UBP ,AUD_FSD611_UBM ,AUD_FSD611_UAs  )      VALUES (:new.PGCOD,:new.PPCTA,:new.PPEXTE,:new.PPFPAG,:new.PPIMP1,:new.PPIMP10,:new.PPIMP11,:new.PPIMP12,:new.PPIMP13,:new.PPIMP14,:new.PPIMP15,:new.PPIMP16,:new.PPIMP17,:new.PPIMP18,:new.PPIMP19,:new.PPIMP2,:new.PPIMP20,:new.PPIMP3,:new.PPIMP4,:new.PPIMP5,:new.PPIMP6,:new.PPIMP7,:new.PPIMP8,:new.PPIMP9,:new.PPMDA,:new.PPMOD,:new.PPOPER,:new.PPPAP,:new.PPSBOP,:new.PPSUC,:new.PPTIPO,:new.PPTOPE, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'I'); END T_AUD_FSD611_AUD_I;
