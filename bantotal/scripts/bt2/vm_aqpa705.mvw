CREATE MATERIALIZED VIEW VM_AQPA705
REFRESH FORCE ON DEMAND
AS
SELECT /*+parallel(D,16)*/ D.AQPA705FECTRA,
       D.AQPA705CTAORI,
       D.AQPA705AUXV5,
       D.AQPA705CANAL,
       D.AQPA705TIPOPE
FROM AQPA705 D
WHERE AQPA705FECTRA >= TRUNC(SYSDATE-1,'MM')
  AND AQPA705FECTRA <= TRUNC(SYSDATE-1)
  AND AQPA705TIPOPE = 'A';

