CREATE OR REPLACE PROCEDURE AUD_C_SNG056_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_SNG056_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_SNG056_UOn,AUD_SNG056_GuidKey,'SNG056',CONCAT( CONCAT( CONCAT( CONCAT( '|SNG053CMB=', RTRIM(COALESCE(AUD_old_SNG053CMB,AUD_new_SNG053CMB)) ),CONCAT( '|SNG055EMP=', RTRIM(COALESCE(AUD_old_SNG055EMP,AUD_new_SNG055EMP)) ) ),CONCAT( '|SNG056COD=', RTRIM(COALESCE(AUD_old_SNG056COD,AUD_new_SNG056COD)) ) ),'|' ),AUD_SNG056_UBT,AUD_SNG056_UBU,AUD_SNG056_UBM,AUD_SNG056_UBP,AUD_SNG056_UBR,AUD_SNG056_UAs,AUD_SNG056_UBC,AUD_SNG056_UBA,AUD_SNG056_UBS, '', 'BANTOTAL', '', '', '',AUD_SNG056_UT FROM BANTPROD.AUD_SNG056_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG056_UOn AND AUDMstId = AUD_SNG056_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG056_UOn,AUD_SNG056_GuidKey,'SNG053CMB',AUD_old_SNG053CMB,AUD_new_SNG053CMB FROM BANTPROD.AUD_SNG056_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG056_UOn AND AUDMstId = AUD_SNG056_GuidKey AND AUDMstFld = 'SNG053CMB'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG056_UOn,AUD_SNG056_GuidKey,'SNG055CAR',AUD_old_SNG055CAR,AUD_new_SNG055CAR FROM BANTPROD.AUD_SNG056_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG056_UOn AND AUDMstId = AUD_SNG056_GuidKey AND AUDMstFld = 'SNG055CAR'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG056_UOn,AUD_SNG056_GuidKey,'SNG055EMP',AUD_old_SNG055EMP,AUD_new_SNG055EMP FROM BANTPROD.AUD_SNG056_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG056_UOn AND AUDMstId = AUD_SNG056_GuidKey AND AUDMstFld = 'SNG055EMP'); INSERT INTO BANTPROD.AUD005 SELECT AUD_SNG056_UOn,AUD_SNG056_GuidKey,'SNG056COD',AUD_old_SNG056COD,AUD_new_SNG056COD FROM BANTPROD.AUD_SNG056_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_SNG056_UOn AND AUDMstId = AUD_SNG056_GuidKey AND AUDMstFld = 'SNG056COD'); DELETE FROM AUD_SNG056_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_SNG056_UOn AND AUDMstId = AUD_SNG056_GuidKey);  COMMIT ;END IF;END;
