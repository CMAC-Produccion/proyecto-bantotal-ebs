CREATE OR REPLACE PROCEDURE AUD_C_FSR027_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_FSR027_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_FSR027_UOn,AUD_FSR027_GuidKey,'FSR027',CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( '|CTNRO=', RTRIM(COALESCE(AUD_old_CTNRO,AUD_new_CTNRO)) ),CONCAT( '|MODULO=', RTRIM(COALESCE(AUD_old_MODULO,AUD_new_MODULO)) ) ),CONCAT( '|MONEDA=', RTRIM(COALESCE(AUD_old_MONEDA,AUD_new_MONEDA)) ) ),CONCAT( '|PAPEL=', RTRIM(COALESCE(AUD_old_PAPEL,AUD_new_PAPEL)) ) ),CONCAT( '|PGCOD=', RTRIM(COALESCE(AUD_old_PGCOD,AUD_new_PGCOD)) ) ),CONCAT( '|TPFDES=', RTRIM(COALESCE(AUD_old_TPFDES,AUD_new_TPFDES)) ) ),CONCAT( '|TPIZAR=', RTRIM(COALESCE(AUD_old_TPIZAR,AUD_new_TPIZAR)) ) ),CONCAT( '|TPMTO=', RTRIM(COALESCE(AUD_old_TPMTO,AUD_new_TPMTO)) ) ),CONCAT( '|TPPZO=', RTRIM(COALESCE(AUD_old_TPPZO,AUD_new_TPPZO)) ) ),'|' ),AUD_FSR027_UBT,AUD_FSR027_UBU,AUD_FSR027_UBM,AUD_FSR027_UBP,AUD_FSR027_UBR,AUD_FSR027_UAs,AUD_FSR027_UBC,AUD_FSR027_UBA,AUD_FSR027_UBS, '', 'BANTOTAL', '', '', '',AUD_FSR027_UT FROM BANTPROD.AUD_FSR027_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FSR027_UOn AND AUDMstId = AUD_FSR027_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR027_UOn,AUD_FSR027_GuidKey,'CTNRO',AUD_old_CTNRO,AUD_new_CTNRO FROM BANTPROD.AUD_FSR027_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR027_UOn AND AUDMstId = AUD_FSR027_GuidKey AND AUDMstFld = 'CTNRO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR027_UOn,AUD_FSR027_GuidKey,'MODULO',AUD_old_MODULO,AUD_new_MODULO FROM BANTPROD.AUD_FSR027_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR027_UOn AND AUDMstId = AUD_FSR027_GuidKey AND AUDMstFld = 'MODULO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR027_UOn,AUD_FSR027_GuidKey,'MONEDA',AUD_old_MONEDA,AUD_new_MONEDA FROM BANTPROD.AUD_FSR027_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR027_UOn AND AUDMstId = AUD_FSR027_GuidKey AND AUDMstFld = 'MONEDA'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR027_UOn,AUD_FSR027_GuidKey,'PAPEL',AUD_old_PAPEL,AUD_new_PAPEL FROM BANTPROD.AUD_FSR027_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR027_UOn AND AUDMstId = AUD_FSR027_GuidKey AND AUDMstFld = 'PAPEL'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR027_UOn,AUD_FSR027_GuidKey,'PGCOD',AUD_old_PGCOD,AUD_new_PGCOD FROM BANTPROD.AUD_FSR027_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR027_UOn AND AUDMstId = AUD_FSR027_GuidKey AND AUDMstFld = 'PGCOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR027_UOn,AUD_FSR027_GuidKey,'TPFDES',TO_CHAR(AUD_old_TPFDES,'YYYY-MM-DD'),TO_CHAR(AUD_new_TPFDES,'YYYY-MM-DD') FROM BANTPROD.AUD_FSR027_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR027_UOn AND AUDMstId = AUD_FSR027_GuidKey AND AUDMstFld = 'TPFDES'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR027_UOn,AUD_FSR027_GuidKey,'TPIZAR',AUD_old_TPIZAR,AUD_new_TPIZAR FROM BANTPROD.AUD_FSR027_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR027_UOn AND AUDMstId = AUD_FSR027_GuidKey AND AUDMstFld = 'TPIZAR'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR027_UOn,AUD_FSR027_GuidKey,'TPMTO',AUD_old_TPMTO,AUD_new_TPMTO FROM BANTPROD.AUD_FSR027_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR027_UOn AND AUDMstId = AUD_FSR027_GuidKey AND AUDMstFld = 'TPMTO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FSR027_UOn,AUD_FSR027_GuidKey,'TPPZO',AUD_old_TPPZO,AUD_new_TPPZO FROM BANTPROD.AUD_FSR027_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FSR027_UOn AND AUDMstId = AUD_FSR027_GuidKey AND AUDMstFld = 'TPPZO'); I
