CREATE OR REPLACE TRIGGER T_AUD_FST135_AUD_I  AFTER INSERT ON FST135  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FST135_AUDIT ( AUD_new_CMOBS,AUD_new_CMORD,AUD_new_CMSBOR,AUD_new_CMSN1,AUD_new_CMSN2,AUD_new_CMTIMP,AUD_new_CMTOP,AUD_new_COCOD,AUD_new_PGCOD,AUD_new_TRMOD,AUD_new_TRNRO,AUD_new_TRORD, AUD_FST135_GuidKey ,AUD_FST135_UOn,AUD_FST135_UT ,AUD_FST135_UBR ,AUD_FST135_UBC ,AUD_FST135_UBT ,AUD_FST135_UBU ,AUD_FST135_UBA ,AUD_FST135_UBS ,AUD_FST135_UBP ,AUD_FST135_UBM ,AUD_FST135_UAs  )      VALUES (:new.CMOBS,:new.CMORD,:new.CMSBOR,:new.CMSN1,:new.CMSN2,:new.CMTIMP,:new.CMTOP,:new.COCOD,:new.PGCOD,:new.TRMOD,:new.TRNRO,:new.TRORD, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'I'); END T_AUD_FST135_AUD_I;
/

