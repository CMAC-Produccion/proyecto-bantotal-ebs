CREATE OR REPLACE TRIGGER T_AUD_FSD602_AUD_I  AFTER INSERT ON FSD602  FOR EACH ROW    DECLARE	     RequestNumber VARCHAR2(50);      UserName VARCHAR2(50); 	  WrkstName VARCHAR2(50); 	  Server VARCHAR2(50); 	  Application VARCHAR2(50); 	  ProgramName VARCHAR2(50); 	  TransactionNumber VARCHAR2(50); 	  ConnectionId VARCHAR2(50);      ExistConData NUMBER;    BEGIN      SELECT sys_context('USERENV', 'SID') INTO ConnectionId FROM dual;      SELECT COUNT(*) INTO ExistConData FROM DBA_OBJECTS WHERE OWNER=sys_context( 'userenv', 'current_schema' ) AND OBJECT_TYPE = 'TABLE' AND OBJECT_NAME = 'CONDATA';      IF ExistConData > 0 THEN 		 SELECT MAX(Value) INTO RequestNumber FROM CONDATA WHERE Data ='REQUESTID'; 		 SELECT MAX(Value) INTO UserName FROM CONDATA WHERE Data ='USERID'; 		 SELECT MAX(Value) INTO WrkstName FROM CONDATA WHERE Data ='WRKST'; 		 SELECT MAX(Value) INTO Server FROM CONDATA WHERE Data ='SERVER'; 		 SELECT MAX(Value) INTO Application FROM CONDATA WHERE Data ='APPLICATION'; 		 SELECT MAX(Value) INTO ProgramName FROM CONDATA WHERE Data ='PROGRAM';      END IF;     RequestNumber := COALESCE(RequestNumber,'-1');     TransactionNumber := COALESCE(TransactionNumber,'-1');     ConnectionId := COALESCE(ConnectionId,'-1');     SELECT COALESCE(UserName,sys_context('USERENV', 'SESSION_USER'),'NOTDEFINED') INTO UserName FROM dual;     SELECT COALESCE(WrkstName,sys_context('USERENV', 'HOST'),'NOTDEFINED') INTO WrkstName FROM dual;     SELECT COALESCE(Server,sys_context('USERENV', 'SERVER_HOST'),'NOTDEFINED') INTO Server FROM dual;     SELECT COALESCE(Application,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO Application FROM dual;     SELECT COALESCE(ProgramName,sys_context('USERENV', 'ACTION'),'NOTDEFINED') INTO ProgramName FROM dual;      INSERT INTO BANTPROD.AUD_FSD602_AUDIT ( AUD_new_D602CD,AUD_new_D602CO,AUD_new_D602FC,AUD_new_D602MO,AUD_new_D602OR,AUD_new_D602RE,AUD_new_D602SB,AUD_new_D602SU,AUD_new_D602TR,AUD_new_PGCOD,AUD_new_PP1CAP,AUD_new_PP1FECH,AUD_new_PP1ICAP,AUD_new_PP1IINT,AUD_new_PP1IINTM,AUD_new_PP1INT,AUD_new_PP1INTM,AUD_new_PP1INTMEX,AUD_new_PP1INTMMEX,AUD_new_PP1NUMP,AUD_new_PP1SALADE,AUD_new_PP1SALADEM,AUD_new_PP1SALCAP,AUD_new_PP1SALINT,AUD_new_PP1SALINTM,AUD_new_PP1SALMOR,AUD_new_PP1SALMORM,AUD_new_PP1STAT,AUD_new_PPCTA,AUD_new_PPFPAG,AUD_new_PPMDA,AUD_new_PPMOD,AUD_new_PPOPER,AUD_new_PPPAP,AUD_new_PPSBOP,AUD_new_PPSUC,AUD_new_PPTIPO,AUD_new_PPTOPE, AUD_FSD602_GuidKey ,AUD_FSD602_UOn,AUD_FSD602_UT ,AUD_FSD602_UBR ,AUD_FSD602_UBC ,AUD_FSD602_UBT ,AUD_FSD602_UBU ,AUD_FSD602_UBA ,AUD_FSD602_UBS ,AUD_FSD602_UBP ,AUD_FSD602_UBM ,AUD_FSD602_UAs  )      VALUES (:new.D602CD,:new.D602CO,:new.D602FC,:new.D602MO,:new.D602OR,:new.D602RE,:new.D602SB,:new.D602SU,:new.D602TR,:new.PGCOD,:new.PP1CAP,:new.PP1FECH,:new.PP1ICAP,:new.PP1IINT,:new.PP1IINTM,:new.PP1INT,:new.PP1INTM,:new.PP1INTMEX,:new.PP1INTMMEX,:new.PP1NUMP,:new.PP1SALADE,:new.PP1SALADEM,:new.PP1SALCAP,:new.PP1SALINT,:new.PP1SALINTM,:new.PP1SALMOR,:new.PP1SALMORM,:new.PP1STAT,:new.PPCTA,:new.PPFPAG,:new.PPMDA,:new.PPMOD,:new.PPOPER,:new.PPPAP,:new.PPSBOP,:new.PPSUC,:new.PPTIPO,:new.PPTOPE, SYS_GUID(),SYSTIMESTAMP,SYSTIMESTAMP,RequestNumber,ConnectionId,TransactionNumber,UserName,Application,Server,ProgramName,WrkstName,'I'); END T_AUD_FSD602_AUD_I;
