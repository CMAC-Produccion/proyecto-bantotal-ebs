  jaqm6dnum1 NUMBER(10),
  jaqm6dnum2 NUMBER(10),
  jaqm6dimp1 NUMBER(17,2),
  jaqm6dimp2 NUMBER(17,2),
  jaqm6dvar1 VARCHAR2(255),
  jaqm6dvar2 VARCHAR2(255),
  jaqm6dfec1 DATE,
  jaqm6dfec2 DATE
)
;
comment on table JAQM6D
