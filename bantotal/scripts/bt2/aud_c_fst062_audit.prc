CREATE OR REPLACE PROCEDURE AUD_C_FST062_AUDIT AS cnt NUMBER; BEGIN SELECT COUNT(*) INTO cnt FROM BANTPROD.AUD_FST062_AUDIT; IF( cnt > 0 ) THEN INSERT INTO BANTPROD.AUD004 (AUDMstTim, AUDMstId, AUDMstTbl, AUDMstKey, AUDMstTrn, AUDMstUsr, AUDMstWst, AUDMstPgm, AUDMstReq, AUDMstTyp, AUDMstCon, AUDMstApp, AUDMstSrv, AUDMstSes, AUDMstGrp, AUDMstLog, AUDMstCur, AUDMstSnt,AUDMstTic) SELECT AUD_FST062_UOn,AUD_FST062_GuidKey,'FST062',CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( CONCAT( '|PGCOD=', RTRIM(COALESCE(AUD_old_PGCOD,AUD_new_PGCOD)) ),CONCAT( '|TREXT=', RTRIM(COALESCE(AUD_old_TREXT,AUD_new_TREXT)) ) ),CONCAT( '|TRMOD=', RTRIM(COALESCE(AUD_old_TRMOD,AUD_new_TRMOD)) ) ),CONCAT( '|TRNRO=', RTRIM(COALESCE(AUD_old_TRNRO,AUD_new_TRNRO)) ) ),CONCAT( '|TRORD=', RTRIM(COALESCE(AUD_old_TRORD,AUD_new_TRORD)) ) ),'|' ),AUD_FST062_UBT,AUD_FST062_UBU,AUD_FST062_UBM,AUD_FST062_UBP,AUD_FST062_UBR,AUD_FST062_UAs,AUD_FST062_UBC,AUD_FST062_UBA,AUD_FST062_UBS, '', 'BANTOTAL', '', '', '',AUD_FST062_UT FROM BANTPROD.AUD_FST062_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST062_UOn AND AUDMstId = AUD_FST062_GuidKey); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST062_UOn,AUD_FST062_GuidKey,'EXTCOD',AUD_old_EXTCOD,AUD_new_EXTCOD FROM BANTPROD.AUD_FST062_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST062_UOn AND AUDMstId = AUD_FST062_GuidKey AND AUDMstFld = 'EXTCOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST062_UOn,AUD_FST062_GuidKey,'PGCOD',AUD_old_PGCOD,AUD_new_PGCOD FROM BANTPROD.AUD_FST062_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST062_UOn AND AUDMstId = AUD_FST062_GuidKey AND AUDMstFld = 'PGCOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST062_UOn,AUD_FST062_GuidKey,'TREXT',AUD_old_TREXT,AUD_new_TREXT FROM BANTPROD.AUD_FST062_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST062_UOn AND AUDMstId = AUD_FST062_GuidKey AND AUDMstFld = 'TREXT'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST062_UOn,AUD_FST062_GuidKey,'TRMOD',AUD_old_TRMOD,AUD_new_TRMOD FROM BANTPROD.AUD_FST062_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST062_UOn AND AUDMstId = AUD_FST062_GuidKey AND AUDMstFld = 'TRMOD'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST062_UOn,AUD_FST062_GuidKey,'TRNRO',AUD_old_TRNRO,AUD_new_TRNRO FROM BANTPROD.AUD_FST062_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST062_UOn AND AUDMstId = AUD_FST062_GuidKey AND AUDMstFld = 'TRNRO'); INSERT INTO BANTPROD.AUD005 SELECT AUD_FST062_UOn,AUD_FST062_GuidKey,'TRORD',AUD_old_TRORD,AUD_new_TRORD FROM BANTPROD.AUD_FST062_AUDIT WHERE NOT EXISTS (SELECT * FROM BANTPROD.AUD005 WHERE AUDMstTim = AUD_FST062_UOn AND AUDMstId = AUD_FST062_GuidKey AND AUDMstFld = 'TRORD'); DELETE FROM AUD_FST062_AUDIT  WHERE  EXISTS (SELECT * FROM BANTPROD.AUD004 WHERE AUDMstTim = AUD_FST062_UOn AND AUDMstId = AUD_FST062_GuidKey);  COMMIT ;END IF;END;
